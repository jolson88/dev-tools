[general]
status_path = ~/.vdirsyncer/status/

[pair concur_calendar]
a = concur_calendar_local
b = concur_calendar_remote
collections = ["from a", "from b"]

metadata = ["displayname", "color"]

[storage concur_calendar_local]
type = filesystem
path = ~/.calendars/
fileext = .ics

[storage concur_calendar_remote]
type = caldav
read_only = true
url = http://localhost:1080/users/jason.olson@concur.com/calendar/
username = CONCUR\jolson
password.fetch = ["command", "pass", "concur.com/exchange"]
